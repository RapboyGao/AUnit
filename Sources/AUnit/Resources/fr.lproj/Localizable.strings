"pressure.symbol" = "P";
"pressure.shortName" = "Pression";
"pressure.longName" = "Pression";
"pressure.detailedIntroduction" = "La pression est une mesure de la force par unité de surface. Elle est souvent mesurée en pascals (Pa) ou en bars.";

"temperature.symbol" = "T";
