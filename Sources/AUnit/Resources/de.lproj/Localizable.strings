/* General */
"None" = "Keine";

/* Unit Types */
"length.symbol" = "L";
"length.shortName" = "Länge";
"length.longName" = "Längeneinheit";
"length.detailedIntroduction" = "Die Längeneinheit misst die räumliche Ausdehnung eines Objekts. Zu den gängigen Längeneinheiten gehören Meter, Kilometer, Zoll und Fuß.";

"speed.symbol" = "v";
"speed.shortName" = "Geschwindigkeit";
"speed.longName" = "Geschwindigkeitseinheit";
"speed.detailedIntroduction" = "Die Geschwindigkeitseinheit misst, wie schnell sich ein Objekt bewegt. Zu den üblichen Einheiten gehören Meter pro Sekunde (m/s) und Kilometer pro Stunde (km/h).";

"pressure.symbol" = "P";
"pressure.shortName" = "Druck";
"pressure.longName" = "Druckeinheit";
"pressure.detailedIntroduction" = "Die Druckeinheit misst die Kraft pro Flächeneinheit. Gängige Druckeinheiten sind Pascal (Pa), Bar und Pfund pro Quadratzoll (psi).";

"temperature.symbol" = "T";
"temperature.shortName" = "Temperatur";
"temperature.longName" = "Temperatureinheit";
"temperature.detailedIntroduction" = "Die Temperatureinheit misst den Wärmegrad eines Objekts oder eines Raumes. Zu den gängigen Einheiten gehören Celsius (°C), Kelvin (K) und Fahrenheit (°F).";

"temperatureDifference.symbol" = "ΔT";
"temperatureDifference.shortName" = "Temperaturunterschied";
"temperatureDifference.longName" = "Temperaturunterschiedseinheit";
"temperatureDifference.detailedIntroduction" = "Die Temperaturunterschiedseinheit misst die Differenz zwischen zwei Temperaturwerten. Sie wird oft in Klimawissenschaften und Ingenieurwesen verwendet.";

"mass.symbol" = "m";
"mass.shortName" = "Masse";
"mass.longName" = "Masseinheit";
"mass.detailedIntroduction" = "Die Masseinheit misst die Menge an Materie in einem Objekt. Zu den gängigen Masseeinheiten gehören Kilogramm (kg), Gramm (g) und Pfund (lb).";

"volume.symbol" = "V";
"volume.shortName" = "Volumen";
"volume.longName" = "Volumeneinheit";
"volume.detailedIntroduction" = "Die Volumeneinheit misst den Raum, den ein Objekt einnimmt. Gängige Volumeneinheiten sind Liter (L), Milliliter (ml) und Kubikmeter (m³).";

"acceleration.symbol" = "a";
"acceleration.shortName" = "Beschleunigung";
"acceleration.longName" = "Beschleunigungseinheit";
"acceleration.detailedIntroduction" = "Die Beschleunigungseinheit misst die Änderung der Geschwindigkeit pro Zeiteinheit. Sie wird in Meter pro Quadratsekunde (m/s²) angegeben.";

"power.symbol" = "P";
"power.shortName" = "Leistung";
"power.longName" = "Leistungseinheit";
"power.detailedIntroduction" = "Die Leistungseinheit misst die Menge an Energie, die pro Zeiteinheit übertragen wird. Gängige Leistungseinheiten sind Watt (W) und Pferdestärke (PS).";

"angle.symbol" = "θ";
"angle.shortName" = "Winkel";
"angle.longName" = "Winkeleinheit";
"angle.detailedIntroduction" = "Die Winkeleinheit misst die Drehung zwischen zwei Linien, die von einem gemeinsamen Punkt ausgehen. Gängige Winkeleinheiten sind Grad (°) und Radiant (rad).";

"angularVelocity.symbol" = "ω";
"angularVelocity.shortName" = "Winkelgeschwindigkeit";
"angularVelocity.longName" = "Winkelgeschwindigkeitseinheit";
"angularVelocity.detailedIntroduction" = "Die Winkelgeschwindigkeitseinheit misst die Drehgeschwindigkeit. Sie wird in Radiant pro Sekunde (rad/s) angegeben.";

"area.symbol" = "A";
"area.shortName" = "Fläche";
"area.longName" = "Flächeneinheit";
"area.detailedIntroduction" = "Die Flächeneinheit misst die Größe einer zweidimensionalen Oberfläche. Zu den gängigen Flächeneinheiten gehören Quadratmeter (m²) und Hektar.";

"concentration.symbol" = "C";
"concentration.shortName" = "Konzentration";
"concentration.longName" = "Konzentrationseinheit";
"concentration.detailedIntroduction" = "Die Konzentrationseinheit misst die Menge eines Stoffes in einer bestimmten Volumeneinheit. Sie wird oft in Gramm pro Liter (g/L) oder Teile pro Million (ppm) angegeben.";

"time.symbol" = "t";
"time.shortName" = "Zeit";
"time.longName" = "Zeiteinheit";
"time.detailedIntroduction" = "Die Zeiteinheit misst die Dauer von Ereignissen. Gängige Zeiteinheiten sind Sekunden (s), Minuten (min) und Stunden (h).";

"electricChargeCapacity.symbol" = "Q";
"electricChargeCapacity.shortName" = "Elektrische Ladungskapazität";
"electricChargeCapacity.longName" = "Einheit der elektrischen Ladungskapazität";
"electricChargeCapacity.detailedIntroduction" = "Die Einheit der elektrischen Ladungskapazität misst die gespeicherte elektrische Ladung. Sie wird in Amperestunden (Ah) oder Milliamperestunden (mAh) angegeben.";

"electricCurrent.symbol" = "I";
"electricCurrent.shortName" = "Stromstärke";
"electricCurrent.longName" = "Stromstärkeeineinheit";
"electricCurrent.detailedIntroduction" = "Die Stromstärkeeineinheit misst den Fluss elektrischer Ladung. Sie wird in Ampere (A) gemessen.";

"electricPotential.symbol" = "V";
"electricPotential.shortName" = "Spannung";
"electricPotential.longName" = "Spannungseinheit";
"electricPotential.detailedIntroduction" = "Die Spannungseinheit misst den elektrischen Potenzialunterschied zwischen zwei Punkten. Sie wird in Volt (V) gemessen.";

"electricResistance.symbol" = "R";
"electricResistance.shortName" = "Widerstand";
"electricResistance.longName" = "Widerstandseinheit";
"electricResistance.detailedIntroduction" = "Die Widerstandseinheit misst den Widerstand gegen den Fluss elektrischer Ladung. Sie wird in Ohm (Ω) gemessen.";

"energy.symbol" = "E";
"energy.shortName" = "Energie";
"energy.longName" = "Energieeinheit";
"energy.detailedIntroduction" = "Die Energieeinheit misst die Fähigkeit, Arbeit zu verrichten. Gängige Energieeinheiten sind Joule (J) und Kilokalorien (kcal).";

"frequency.symbol" = "f";
"frequency.shortName" = "Frequenz";
"frequency.longName" = "Frequenzeinheit";
"frequency.detailedIntroduction" = "Die Frequenzeinheit misst die Anzahl der Wiederholungen eines Ereignisses pro Zeiteinheit. Sie wird in Hertz (Hz) gemessen.";

"fuelEfficiency.symbol" = "FE";
"fuelEfficiency.shortName" = "Kraftstoffeffizienz";
"fuelEfficiency.longName" = "Kraftstoffeffizienzeinheit";
"fuelEfficiency.detailedIntroduction" = "Die Kraftstoffeffizienzeinheit misst, wie weit ein Fahrzeug mit einer bestimmten Menge Kraftstoff fahren kann. Gängige Einheiten sind Liter pro 100 Kilometer (L/100 km) und Meilen pro Gallone (mpg).";

"data.symbol" = "D";
"data.shortName" = "Daten";
"data.longName" = "Dateneinheit";
"data.detailedIntroduction" = "Die Dateneinheit misst die Menge an digitalen Informationen. Zu den gängigen Dateneinheiten gehören Bytes, Kilobytes (KB) und Megabytes (MB).";

/* Units */
"feet.symbol" = "ft";
"feet.shortName" = "Fuß";
"feet.longName" = "Fuß";
"feet.detailedIntroduction" = "Der Fuß ist eine Längeneinheit, die im angelsächsischen Maßsystem verwendet wird. 1 Fuß entspricht 0,3048 Meter.";

"nauticalMiles.symbol" = "NM";
"nauticalMiles.shortName" = "Seemeile";
"nauticalMiles.longName" = "Seemeile";
"nauticalMiles.detailedIntroduction" = "Die Seemeile ist eine Längeneinheit, die in der Schifffahrt und Luftfahrt verwendet wird. Eine Seemeile entspricht etwa 1,852 Kilometer.";

"meters.symbol" = "m";
"meters.shortName" = "Meter";
"meters.longName" = "Meter";
"meters.detailedIntroduction" = "Der Meter ist die Basiseinheit der Länge im internationalen Einheitensystem (SI). 1 Meter ist definiert als die Strecke, die Licht im Vakuum in 1/299.792.458 Sekunden zurücklegt.";

"kilometers.symbol" = "km";
"kilometers.shortName" = "Kilometer";
"kilometers.longName" = "Kilometer";
"kilometers.detailedIntroduction" = "Der Kilometer ist eine Längeneinheit, die 1.000 Metern entspricht. Es wird häufig verwendet, um Entfernungen auf der Erde zu messen.";

"miles.symbol" = "mi";
"miles.shortName" = "Meile";
"miles.longName" = "Meile";
"miles.detailedIntroduction" = "Die Meile ist eine Längeneinheit, die hauptsächlich in den USA verwendet wird. 1 Meile entspricht 1.609,344 Metern.";

"megameters.symbol" = "Mm";
"megameters.shortName" = "Megameter";
"megameters.longName" = "Megameter";
"megameters.detailedIntroduction" = "Der Megameter ist eine Längeneinheit, die einer Million Metern entspricht. Es wird selten verwendet und findet hauptsächlich in wissenschaftlichen Kontexten Anwendung.";

"hectometers.symbol" = "hm";
"hectometers.shortName" = "Hektometer";
"hectometers.longName" = "Hektometer";
"hectometers.detailedIntroduction" = "Der Hektometer ist eine Längeneinheit, die 100 Metern entspricht. Es wird hauptsächlich in der Geographie und Landwirtschaft verwendet.";

"decameters.symbol" = "dam";
"decameters.shortName" = "Dekameter";
"decameters.longName" = "Dekameter";
"decameters.detailedIntroduction" = "Der Dekameter ist eine Längeneinheit, die 10 Metern entspricht. Es wird selten verwendet und findet hauptsächlich in speziellen wissenschaftlichen Kontexten Anwendung.";

"decimeters.symbol" = "dm";
"decimeters.shortName" = "Dezimeter";
"decimeters.longName" = "Dezimeter";
"decimeters.detailedIntroduction" = "Der Dezimeter ist eine Längeneinheit, die 0,1 Metern entspricht. Es wird selten verwendet und findet hauptsächlich in der Bildung und Wissenschaft Anwendung.";

"centimeters.symbol" = "cm";
"centimeters.shortName" = "Zentimeter";
"centimeters.longName" = "Zentimeter";
"centimeters.detailedIntroduction" = "Der Zentimeter ist eine Längeneinheit, die 0,01 Metern entspricht. Es wird häufig in Alltagssituationen und in der Wissenschaft verwendet.";

"millimeters.symbol" = "mm";
"millimeters.shortName" = "Millimeter";
"millimeters.longName" = "Millimeter";
"millimeters.detailedIntroduction" = "Der Millimeter ist eine Längeneinheit, die 0,001 Metern entspricht. Es wird häufig in der Technik, Bauwesen und Wissenschaft verwendet.";

"micrometers.symbol" = "µm";
"micrometers.shortName" = "Mikrometer";
"micrometers.longName" = "Mikrometer";
"micrometers.detailedIntroduction" = "Der Mikrometer ist eine Längeneinheit, die einem Millionstel Meter entspricht. Es wird häufig in der Mikroskopie und Materialwissenschaft verwendet.";

"nanometers.symbol" = "nm";
"nanometers.shortName" = "Nanometer";
"nanometers.longName" = "Nanometer";
"nanometers.detailedIntroduction" = "Der Nanometer ist eine Längeneinheit, die einem Milliardstel Meter entspricht. Es wird häufig in der Nanotechnologie und Physik verwendet.";

"picometers.symbol" = "pm";
"picometers.shortName" = "Pikometer";
"picometers.longName" = "Pikometer";
"picometers.detailedIntroduction" = "Der Pikometer ist eine Längeneinheit, die einem Billionstel Meter entspricht. Es wird hauptsächlich in der Quantenphysik und Chemie verwendet.";

"inches.symbol" = "in";
"inches.shortName" = "Zoll";
"inches.longName" = "Zoll";
"inches.detailedIntroduction" = "Der Zoll ist eine Längeneinheit, die im angelsächsischen Maßsystem verwendet wird. 1 Zoll entspricht 2,54 Zentimetern.";

"yards.symbol" = "yd";
"yards.shortName" = "Yard";
"yards.longName" = "Yard";
"yards.detailedIntroduction" = "Der Yard ist eine Längeneinheit, die im angelsächsischen Maßsystem verwendet wird. 1 Yard entspricht 0,9144 Metern.";

"scandinavianMiles.symbol" = "smi";
"scandinavianMiles.shortName" = "Skandinavische Meile";
"scandinavianMiles.longName" = "Skandinavische Meile";
"scandinavianMiles.detailedIntroduction" = "Die skandinavische Meile ist eine Längeneinheit, die in Skandinavien verwendet wird. Eine skandinavische Meile entspricht 10 Kilometern.";

"lightyears.symbol" = "ly";
"lightyears.shortName" = "Lichtjahr";
"lightyears.longName" = "Lichtjahr";
"lightyears.detailedIntroduction" = "Das Lichtjahr ist eine Längeneinheit, die die Entfernung misst, die Licht in einem Jahr im Vakuum zurücklegt. Ein Lichtjahr entspricht etwa 9,461 Billionen Kilometern.";

"fathoms.symbol" = "fth";
"fathoms.shortName" = "Faden";
"fathoms.longName" = "Faden";
"fathoms.detailedIntroduction" = "Der Faden ist eine Längeneinheit, die in der Schifffahrt verwendet wird, um die Wassertiefe zu messen. Ein Faden entspricht 1,8288 Metern.";

"furlongs.symbol" = "fur";
"furlongs.shortName" = "Furlong";
"furlongs.longName" = "Furlong";
"furlongs.detailedIntroduction" = "Das Furlong ist eine Längeneinheit, die hauptsächlich in der Pferderennsport verwendet wird. Ein Furlong entspricht 201,168 Metern.";

"astronomicalUnits.symbol" = "AU";
"astronomicalUnits.shortName" = "Astronomische Einheit";
"astronomicalUnits.longName" = "Astronomische Einheit";
"astronomicalUnits.detailedIntroduction" = "Die astronomische Einheit ist eine Längeneinheit, die verwendet wird, um Entfernungen innerhalb des Sonnensystems zu messen. Eine astronomische Einheit entspricht etwa 149,597,870.7 Kilometern.";

"parsecs.symbol" = "pc";
"parsecs.shortName" = "Parsec";
"parsecs.longName" = "Parsec";
"parsecs.detailedIntroduction" = "Das Parsec ist eine Längeneinheit, die in der Astronomie verwendet wird, um große Entfernungen zu messen. Ein Parsec entspricht etwa 3,26 Lichtjahren.";

/* Speed */
"knots.symbol" = "kn";
"knots.shortName" = "Knoten";
"knots.longName" = "Knoten";
"knots.detailedIntroduction" = "Der Knoten ist eine Geschwindigkeitseinheit, die in der Schifffahrt und Luftfahrt verwendet wird. Ein Knoten entspricht etwa 1,852 Kilometer pro Stunde.";

"metersPerSecond.symbol" = "m/s";
"metersPerSecond.shortName" = "Meter pro Sekunde";
"metersPerSecond.longName" = "Meter pro Sekunde";
"metersPerSecond.detailedIntroduction" = "Meter pro Sekunde ist die Basiseinheit der Geschwindigkeit im internationalen Einheitensystem (SI). Es misst die zurückgelegte Strecke in Metern pro Sekunde.";

"feetPerMinute.symbol" = "ft/min";
"feetPerMinute.shortName" = "Fuß pro Minute";
"feetPerMinute.longName" = "Fuß pro Minute";
"feetPerMinute.detailedIntroduction" = "Fuß pro Minute ist eine Geschwindigkeitseinheit, die hauptsächlich in der Luftfahrt verwendet wird. Ein Fuß pro Minute entspricht 0,00508 Meter pro Sekunde.";

"kilometersPerHour.symbol" = "km/h";
"kilometersPerHour.shortName" = "Kilometer pro Stunde";
"kilometersPerHour.longName" = "Kilometer pro Stunde";
"kilometersPerHour.detailedIntroduction" = "Kilometer pro Stunde ist eine Geschwindigkeitseinheit, die die zurückgelegte Strecke in Kilometern pro Stunde misst. Es wird häufig im Straßenverkehr verwendet.";

"feetPerSecond.symbol" = "ft/s";
"feetPerSecond.shortName" = "Fuß pro Sekunde";
"feetPerSecond.longName" = "Fuß pro Sekunde";
"feetPerSecond.detailedIntroduction" = "Fuß pro Sekunde ist eine Geschwindigkeitseinheit, die hauptsächlich in den USA verwendet wird. Ein Fuß pro Sekunde entspricht 0,3048 Meter pro Sekunde.";

"milesPerHour.symbol" = "mph";
"milesPerHour.shortName" = "Meilen pro Stunde";
"milesPerHour.longName" = "Meilen pro Stunde";
"milesPerHour.detailedIntroduction" = "Meilen pro Stunde ist eine Geschwindigkeitseinheit, die hauptsächlich in den USA verwendet wird. Eine Meile pro Stunde entspricht etwa 1,60934 Kilometer pro Stunde.";

"inchesPerSecond.symbol" = "in/s";
"inchesPerSecond.shortName" = "Zoll pro Sekunde";
"inchesPerSecond.longName" = "Zoll pro Sekunde";
"inchesPerSecond.detailedIntroduction" = "Zoll pro Sekunde ist eine Geschwindigkeitseinheit, die hauptsächlich in den USA verwendet wird. Ein Zoll pro Sekunde entspricht 0,0254 Meter pro Sekunde.";

"yardsPerSecond.symbol" = "yd/s";
"yardsPerSecond.shortName" = "Yard pro Sekunde";
"yardsPerSecond.longName" = "Yard pro Sekunde";
"yardsPerSecond.detailedIntroduction" = "Yard pro Sekunde ist eine Geschwindigkeitseinheit, die hauptsächlich in den USA verwendet wird. Ein Yard pro Sekunde entspricht 0,9144 Meter pro Sekunde.";

/* Pressure */
"hectopascals.symbol" = "hPa";
"hectopascals.shortName" = "Hektopascal";
"hectopascals.longName" = "Hektopascal";
"hectopascals.detailedIntroduction" = "Hektopascal ist eine Druckeinheit im internationalen Einheitensystem (SI). Ein Hektopascal entspricht 100 Pascal.";

"inchesOfMercury.symbol" = "inHg";
"inchesOfMercury.shortName" = "Zoll Quecksilber";
"inchesOfMercury.longName" = "Zoll Quecksilber";
"inchesOfMercury.detailedIntroduction" = "Zoll Quecksilber ist eine Druckeinheit, die in der Luftfahrt und Meteorologie verwendet wird. Ein Zoll Quecksilber entspricht etwa 3386,39 Pascal.";

"millimetersOfMercury.symbol" = "mmHg";
"millimetersOfMercury.shortName" = "Millimeter Quecksilber";
"millimetersOfMercury.longName" = "Millimeter Quecksilber";
"millimetersOfMercury.detailedIntroduction" = "Millimeter Quecksilber ist eine Druckeinheit, die in der Medizin und Meteorologie verwendet wird. Ein Millimeter Quecksilber entspricht etwa 133,322 Pascal.";

"newtonsPerMetersSquared.symbol" = "N/m²";
"newtonsPerMetersSquared.shortName" = "Newton pro Quadratmeter";
"newtonsPerMetersSquared.longName" = "Newton pro Quadratmeter";
"newtonsPerMetersSquared.detailedIntroduction" = "Newton pro Quadratmeter ist eine Druckeinheit im internationalen Einheitensystem (SI). Ein Newton pro Quadratmeter entspricht einem Pascal.";

"gigapascals.symbol" = "GPa";
"gigapascals.shortName" = "Gigapascal";
"gigapascals.longName" = "Gigapascal";
"gigapascals.detailedIntroduction" = "Gigapascal ist eine Druckeinheit im internationalen Einheitensystem (SI). Ein Gigapascal entspricht 1.000.000.000 Pascal.";

"megapascals.symbol" = "MPa";
"megapascals.shortName" = "Megapascal";
"megapascals.longName" = "Megapascal";
"megapascals.detailedIntroduction" = "Megapascal ist eine Druckeinheit im internationalen Einheitensystem (SI). Ein Megapascal entspricht 1.000.000 Pascal.";

"kilopascals.symbol" = "kPa";
"kilopascals.shortName" = "Kilopascal";
"kilopascals.longName" = "Kilopascal";
"kilopascals.detailedIntroduction" = "Kilopascal ist eine Druckeinheit im internationalen Einheitensystem (SI). Ein Kilopascal entspricht 1.000 Pascal.";

"bars.symbol" = "bar";
"bars.shortName" = "Bar";
"bars.longName" = "Bar";
"bars.detailedIntroduction" = "Bar ist eine Druckeinheit, die in Europa häufig verwendet wird. Ein Bar entspricht 100.000 Pascal.";

"millibars.symbol" = "mbar";
"millibars.shortName" = "Millibar";
"millibars.longName" = "Millibar";
"millibars.detailedIntroduction" = "Millibar ist eine Druckeinheit, die häufig in der Meteorologie verwendet wird. Ein Millibar entspricht 100 Pascal.";

"poundsForcePerSquareInch.symbol" = "psi";
"poundsForcePerSquareInch.shortName" = "Pfund pro Quadratzoll";
"poundsForcePerSquareInch.longName" = "Pfund pro Quadratzoll";
"poundsForcePerSquareInch.detailedIntroduction" = "Pfund pro Quadratzoll ist eine Druckeinheit, die in den USA verwendet wird. Ein Pfund pro Quadratzoll entspricht etwa 6.894,76 Pascal.";

/* Temperature */
"kelvin.symbol" = "K";
"kelvin.shortName" = "Kelvin";
"kelvin.longName" = "Kelvin";
"kelvin.detailedIntroduction" = "Kelvin ist die Basiseinheit der Temperatur im internationalen Einheitensystem (SI). Es misst die thermodynamische Temperatur und beginnt bei absolutem Nullpunkt.";

"celsius.symbol" = "°C";
"celsius.shortName" = "Celsius";
"celsius.longName" = "Celsius";
"celsius.detailedIntroduction" = "Celsius ist eine Temperatureinheit, die hauptsächlich in Europa verwendet wird. Der Nullpunkt entspricht dem Gefrierpunkt von Wasser und 100 °C entspricht dem Siedepunkt.";

"fahrenheit.symbol" = "°F";
"fahrenheit.shortName" = "Fahrenheit";
"fahrenheit.longName" = "Fahrenheit";
"fahrenheit.detailedIntroduction" = "Fahrenheit ist eine Temperatureinheit, die hauptsächlich in den USA verwendet wird. Der Nullpunkt entspricht der Temperatur von einer Mischung aus Eis und Salz.";

"rankine.symbol" = "°R";
"rankine.shortName" = "Rankine";
"rankine.longName" = "Rankine";
"rankine.detailedIntroduction" = "Rankine ist eine Temperatureinheit, die hauptsächlich in der Thermodynamik verwendet wird. Sie basiert auf dem Fahrenheit-Skala, wobei der absolute Nullpunkt 0 °R ist.";

