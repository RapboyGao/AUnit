"celsiusDelta.symbol" = "Δ°C";
"celsiusDelta.shortName" = "Celsiusdifferenz";
"celsiusDelta.longName" = "Celsiusdifferenz";
"celsiusDelta.detailedIntroduction" = "Celsiusdifferenz misst die Differenz zwischen zwei Temperaturen in Grad Celsius. Sie wird häufig in der Wissenschaft und Technik verwendet.";

"fahrenheitDelta.symbol" = "Δ°F";
"fahrenheitDelta.shortName" = "Fahrenheitdifferenz";
"fahrenheitDelta.longName" = "Fahrenheitdifferenz";
"fahrenheitDelta.detailedIntroduction" = "Fahrenheitdifferenz misst die Differenz zwischen zwei Temperaturen in Grad Fahrenheit. Sie wird häufig in der Wissenschaft und Technik verwendet.";

/* Mass */
"metricTons.symbol" = "t";
"metricTons.shortName" = "Tonne";
"metricTons.longName" = "Tonne";
"metricTons.detailedIntroduction" = "Die Tonne ist eine Masseinheit, die 1.000 Kilogramm entspricht. Sie wird häufig in der Industrie und im Handel verwendet.";

"shortTons.symbol" = "st";
"shortTons.shortName" = "Kurze Tonne";
"shortTons.longName" = "Kurze Tonne";
"shortTons.detailedIntroduction" = "Die kurze Tonne ist eine Masseinheit, die hauptsächlich in den USA verwendet wird. Eine kurze Tonne entspricht 2.000 Pfund oder etwa 907,185 Kilogramm.";

"kilograms.symbol" = "kg";
"kilograms.shortName" = "Kilogramm";
"kilograms.longName" = "Kilogramm";
"kilograms.detailedIntroduction" = "Das Kilogramm ist die Basiseinheit der Masse im internationalen Einheitensystem (SI). Ein Kilogramm ist definiert als die Masse eines internationalen Prototyps, der in Paris aufbewahrt wird.";

"kilopounds.symbol" = "klb";
"kilopounds.shortName" = "Kilopfund";
"kilopounds.longName" = "Kilopfund";
"kilopounds.detailedIntroduction" = "Das Kilopfund ist eine Masseinheit, die hauptsächlich in der Industrie verwendet wird. Ein Kilopfund entspricht 1.000 Pfund oder etwa 453,592 Kilogramm.";

"pounds.symbol" = "lb";
"pounds.shortName" = "Pfund";
"pounds.longName" = "Pfund";
"pounds.detailedIntroduction" = "Das Pfund ist eine Masseinheit, die hauptsächlich in den USA verwendet wird. Ein Pfund entspricht etwa 0,453592 Kilogramm.";

"ounces.symbol" = "oz";
"ounces.shortName" = "Unze";
"ounces.longName" = "Unze";
"ounces.detailedIntroduction" = "Die Unze ist eine Masseinheit, die hauptsächlich in den USA verwendet wird. Eine Unze entspricht etwa 28,3495 Gramm.";

"grams.symbol" = "g";
"grams.shortName" = "Gramm";
"grams.longName" = "Gramm";
"grams.detailedIntroduction" = "Das Gramm ist eine Masseinheit, die 1/1000 Kilogramm entspricht. Es wird häufig in der Wissenschaft und im Handel verwendet.";

"decigrams.symbol" = "dg";
"decigrams.shortName" = "Dezigramm";
"decigrams.longName" = "Dezigramm";
